{
  "name": "xylemical/composer-dev-bundle",
  "type": "library",
  "license": "MIT",
  "description": "Provides support for development.",
  "homepage": "https://github.com/xylemical/composer-dev-bundle",
  "autoload": {
    "psr-4": {
      "Xylemical\\Development\\": "src/"
    }
  },
  "require": {
    "php": ">7.4",
    "composer-plugin-api": ">2.0",
    "phpunit/phpunit": ">9.3",
    "phpspec/prophecy-phpunit": ">2.0",
    "phpstan/phpstan": ">0.12.48",
    "drupal/coder": ">8.3",
    "squizlabs/php_codesniffer": ">3.5",
    "dealerdirect/phpcodesniffer-composer-installer": ">0.7.0",
    "phpstan/extension-installer": ">1.1",
    "jangregor/phpstan-prophecy": ">1.0"
  },
  "require-dev": {
    "composer/composer": ">2.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
